

if game.Players.LocalPlayer.Character["International Fedora"] then

game.Players.LocalPlayer.Character["International Fedora"].Name = "InternationalFedora"

elseif game.Players.LocalPlayer.Character["InternationalFedora"] then

game.Players.LocalPlayer.Character["InternationalFedora"].Name = "InternationalFedora"

end

wait(0.0)

if game.Players.LocalPlayer.Character["International Fedora"] then

game.Players.LocalPlayer.Character["International Fedora"].Name = "InternationalFedora"

elseif game.Players.LocalPlayer.Character["InternationalFedora"] then

game.Players.LocalPlayer.Character["InternationalFedora"].Name = "InternationalFedora"

end

wait(0.0)

if game.Players.LocalPlayer.Character:FindFirstChild("InternationalFedora") then

game.Players.LocalPlayer.Character:FindFirstChild("InternationalFedora").Name = "1"

end

wait(0.0)

if game.Players.LocalPlayer.Character:FindFirstChild("InternationalFedora") then

game.Players.LocalPlayer.Character:FindFirstChild("InternationalFedora").Name = "2"

end

wait(0.0)

if game.Players.LocalPlayer.Character:FindFirstChild("InternationalFedora") then

game.Players.LocalPlayer.Character:FindFirstChild("InternationalFedora").Name = "3"

end

wait(0.0)

if game.Players.LocalPlayer.Character:FindFirstChild("InternationalFedora") then

game.Players.LocalPlayer.Character:FindFirstChild("InternationalFedora").Name = "4"

end







local Plr = game:GetService("Players").LocalPlayer -- you

local Player = game:GetService("Players").LocalPlayer-- you

local Character = Player.Character-- your character

local function align(i,v) -- align function

local att0 = Instance.new("Attachment", i)

att0.Position = Vector3.new(0,0,0)



local AP = Instance.new("AlignPosition", i)

AP.Attachment0 = att0

AP.Attachment1 = att1

AP.RigidityEnabled = false

AP.ReactionForceEnabled = false

AP.ApplyAtCenterOfMass = true

AP.MaxForce = 9999999

AP.MaxVelocity = math.huge

AP.Responsiveness = 1000000000000

local AO = Instance.new("AlignOrientation", i)

AO.Attachment0 = att0

AO.Attachment1 = att1

AO.ReactionTorqueEnabled = true

AO.PrimaryAxisOnly = false

AO.MaxTorque = 9999999

AO.MaxAngularVelocity = math.huge

AO.Responsiveness = 10000000000000

end



local Plr = game:GetService("Players").LocalPlayer

local Player = game:GetService("Players").LocalPlayer

local Character = Player.Character









local Hats = {

    xd = Character:WaitForChild("1"), -- the hat/body limb name



}

Hats.xd.Handle.AccessoryWeld:Remove()

Hats.xd.Handle.SpecialMesh:Remove()

local function align(i,v)

local att0 = Instance.new("Attachment", i)

att0.Position = Vector3.new(0,0,0) 

local att1 = Instance.new("Attachment", v)

att1.Position = Vector3.new(0.52,0.3,-0.5)-- literally the same thing

			game:GetService("RunService").Heartbeat:Connect(function()
att1.Rotation = att1.Rotation + Vector3.new(70,0,0)--rotation

end)

local AP = Instance.new("AlignPosition", i)

AP.Attachment0 = att0

AP.Attachment1 = att1

AP.RigidityEnabled = false

AP.ReactionForceEnabled = false

AP.ApplyAtCenterOfMass = true

AP.MaxForce = 9999999

AP.MaxVelocity = math.huge

AP.Responsiveness = 70

local AO = Instance.new("AlignOrientation", i)

AO.Attachment0 = att0

AO.Attachment1 = att1

AO.ReactionTorqueEnabled = true

AO.PrimaryAxisOnly = false

AO.MaxTorque = 100000000000

AO.MaxAngularVelocity = math.huge

AO.Responsiveness = 0

end

align(Hats.xd.Handle, Character["Torso"])

Character.HumanoidRootPart.CanCollide = false

Character.Torso.CanCollide = false





